@model Business.Model.Models.Navigation_S
@{
    ViewBag.Title = "导航列表";
    Layout = "~/Views/Shared/_TreeList.cshtml";

    ViewBag.Modules = "Navigation";
    ViewBag.Ext = "List";
}
@section HeadContent {
    <script>
        var listObj = null;

        function updateNode(type, data) {
            if (listObj) {
                listObj.updateNode(type, data);
            }
        }

    </script>
}

@section HiddenArea {
    @Html.HiddenFor(c => c.N_COLUMN_ID)
}

